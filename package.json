{
  "name": "editorconfig",
  "version": "2.0.0",
  "description": "Helps developers maintain consistent coding styles between different editors",
  "license": "MIT",
  "repository": "sindresorhus/atom-editorconfig",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "private": true,
  "engines": {
    "atom": ">=1.0.11"
  },
  "scripts": {
    "test": "xo && apm test"
  },
  "keywords": [
    "editorconfig",
    "format",
    "code",
    "style",
    "code-style"
  ],
  "dependencies": {
    "editorconfig": "^0.13.2",
    "lazy-req": "^1.1.0",
    "pify": "^2.3.0"
  },
  "devDependencies": {
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "globals": [
      "atom",
      "document"
    ],
    "ignores": [
      "atom/**"
    ],
    "rules": {
      "import/prefer-default-export": "off"
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
